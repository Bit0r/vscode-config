{
	//"http.proxy": "http://localhost:8800",
	//"http.proxySupport": "on",
	// explorer
	"workbench.colorTheme": "Flexoki Dark Green",
	"explorer.confirmDelete": false,
	"explorer.confirmDragAndDrop": false,
	"explorer.incrementalNaming": "smart",
	"window.restoreWindows": "none",
	"window.customMenuBarAltFocus": false,
	"window.enableMenuBarMnemonics": false,
	"window.titleBarStyle": "custom",
	"window.dialogStyle": "custom",
	"vsicons.dontShowNewVersionMessage": true,
	"workbench.iconTheme": "vscode-icons",
	"workbench.quickOpen.closeOnFocusLost": false,
	"security.workspace.trust.untrustedFiles": "open",
	"explorer.fileNesting.enabled": true,
	"explorer.fileNesting.expand": false,
	// Outline Map
	"editor.minimap.enabled": false,
	"outline-map.follow": "viewport",
	// Bookmarks
	"bookmarks.saveBookmarksInProject": true,
	"bookmarks.useWorkaroundForFormatters": true,
	// update
	"update.mode": "manual",
	"update.showReleaseNotes": false,
	"workbench.enableExperiments": false,
	// exclude
	"files.exclude": {
		"**/.oxen": true,
		"**/wandb": true
	},
	"search.exclude": {
		"**/.vscode/": true,
		"**/tsconfig*": true
	},
	"files.watcherExclude": {
		"**/composer.lock": true,
		"**/dist/**": true,
		"**/pnpm-lock.yaml": true,
		"**/polyfill*": true,
		"**/vendor/**": true,
		"**/yarn.lock": true,
		"**/*.class": true
	},
	// file ext association
	"files.associations": {
		"*.ssv": "csv (whitespace)",
		"*.psv": "csv (pipe)",
		"*.usv": "dynamic csv",
		"*.ddb": "json",
		"*.cnf": "properties",
		"*rc": "properties",
		"*config": "properties",
		"*env": "properties",
		"*ignore": "ignore"
	},
	// editor
	"window.zoomLevel": 0.4,
	"editor.tabSize": 2,
	"editor.fontSize": 22,
	"editor.fontLigatures": true,
	//"editor.fontLigatures": "'calt', 'liga', 'dlig', 'ss01', 'ss02', 'ss03', 'ss04', 'ss06', 'ss07', 'ss08'",
	"editor.fontFamily": "Fira Code Light, Noto Sans Mono CJK SC Light, monospace",
	"editor.renderWhitespace": "boundary",
	"editor.renderControlCharacters": true,
	"editor.linkedEditing": true,
	"editor.stickyScroll.enabled": false,
	"editor.largeFileOptimizations": false,
	"editor.inlineSuggest.showToolbar": "always",
	"editor.experimentalWhitespaceRendering": "font",
	"editor.suggest.snippetsPreventQuickSuggestions": false,
	"editor.codeActionsOnSave": {
		"source.organizeImports": "explicit",
	},
	"workbench.editor.tabSizing": "shrink",
	"workbench.editor.wrapTabs": true,
	"workbench.editor.enablePreview": false,
	"interactiveWindow.executeWithShiftEnter": true,
	"workbench.editorAssociations": {
		"*.ipynb": "jupyter-notebook"
	},
	"workbench.localHistory.enabled": false,
	// AI
	"chat.editor.fontSize": 22,
	"editor.inlineSuggest.enabled": true,
	"github.copilot.enable": {
		"*": true,
		"yaml": true,
		"plaintext": true,
		"markdown": true,
		"quarto": true
	},
	"aide.readClipboardImage": true,
	"aide.openaiBaseUrl": "https://40.chatgptsb.net/v1",
	"aide.openaiKey": "sk-7qPBOvBZiTITUOG76604612302B84cDb86A2A456B620262a",
	// terminal
	"debug.console.fontSize": 20,
	"debug.console.fontFamily": "Fira Code Light, Noto Sans Mono CJK SC Light, monospace",
	"terminal.integrated.fontSize": 20,
	"terminal.integrated.cursorStyle": "line",
	"terminal.integrated.allowChords": false,
	"terminal.integrated.sendKeybindingsToShell": true,
	"terminal.integrated.inheritEnv": false,
	"terminal.integrated.enablePersistentSessions": false,
	"terminal.integrated.persistentSessionReviveProcess": "never",
	"terminal.integrated.shellIntegration.decorationsEnabled": "both",
	"terminal.integrated.enableMultiLinePasteWarning": "never",
	"terminal.integrated.localEchoExcludePrograms": [
		"vim",
		"vi",
		"nano",
		"tmux",
		"micro",
		"editor"
	],
	// format
	"files.eol": "\n",
	"editor.insertSpaces": false,
	"editor.formatOnSave": true,
	//"editor.defaultFormatter": "dprint.dprint",
	"files.insertFinalNewline": true,
	"files.trimFinalNewlines": true,
	"files.trimTrailingWhitespace": true,
	//"notebook.formatOnSave.enabled": true,
	"notebook.formatOnCellExecution": true,
	"dprint.verbose": true,
	"dprint.experimentalLsp": true,
	// Git
	"git.enableSmartCommit": true,
	"conventionalCommits.autoCommit": false,
	"conventionalCommits.emojiFormat": "emoji",
	// CodeSnap
	"codesnap.showWindowControls": false,
	"codesnap.target": "window",
	// CSV
	"[dynamic csv]": {},
	"[csv][dynamic csv][tsv][csv (semicolon)][csv (whitespace)]": {
		"editor.stickyScroll.enabled": true
	},
	// rsync
	"sync-rsync.onSaveIndividual": true,
	//"sync-rsync.shell": "sshpass -f ~/.ssh/vm.pass ssh vm",
	"sync-rsync.exclude": [
		".git",
		".vscode",
		".svn"
	],
	// Remote
	"remote.autoForwardPorts": false,
	"remote.autoForwardPortsSource": "process",
	"remote.SSH.lockfilesInTmp": true,
	//"remote.SSH.showLoginTerminal": true,
	"remote.SSH.useLocalServer": false,
	"remote.SSH.remoteServerListenOnSocket": true, // 打开此项可以提高安全性，但是可能会导致autodl连接失败
	"remote.SSH.enableX11Forwarding": false,
	//"remote.downloadExtensionsLocally": true, // 本地下载扩展，避免网络问题
	//"remote.SSH.localServerDownload": "always", // 本地下载服务器，避免网络问题，但有 bug: https://github.com/microsoft/vscode-remote-release/issues/9246
	"remote.SSH.remotePlatform": {
		"*": "linux",
		"zw403-*": "linux",
		"autodl-*": "linux"
	},
	// C
	"C_Cpp.clang_format_path": "/usr/bin/clang-format",
	"cmake.configureOnOpen": false,
	"cmake.enabledOutputParsers": [
		"cmake",
		"gcc",
		"gnuld"
	],
	// dotnet
	"dotnetAcquisitionExtension.existingDotnetPath": [
		{
			"extensionId": "msazurermtools.azurerm-vscode-tools",
			"path": "dotnet"
		}
	],
	// CMake
	"cmake.languageSupport.dotnetPath": "dotnet",
	// HTML/CSS/SCSS
	"html.format.wrapLineLength": 60,
	"html.format.templating": true,
	"html.format.indentHandlebars": true,
	"open-php-html-js-in-browser.customBrowserPath": "/usr/bin/vivaldi",
	"open-php-html-js-in-browser.selectedBrowser": "Custom...",
	"css.lint.duplicateProperties": "warning",
	"css.lint.ieHack": "warning",
	"css.lint.importStatement": "warning",
	"css.lint.unknownVendorSpecificProperties": "warning",
	"scss.lint.duplicateProperties": "warning",
	"scss.lint.ieHack": "warning",
	"scss.lint.unknownVendorSpecificProperties": "warning",
	// Emmet
	"emmet.variables": {
		"lang": "zh-CN"
	},
	"emmet.extensionsPath": [
		"./.vscode/emmet/"
	],
	"emmet.includeLanguages": {
		"svelte": "html"
		// "vue": "html",
	},
	"npm.packageManager": "pnpm",
	// Vue
	"javascript.format.semicolons": "remove",
	"typescript.format.semicolons": "remove",
	"javascript.preferences.quoteStyle": "single",
	"typescript.preferences.quoteStyle": "single",
	"javascript.updateImportsOnFileMove.enabled": "never",
	"typescript.updateImportsOnFileMove.enabled": "never",
	"vue.autoInsert.dotValue": true,
	"vue.codeActions.enabled": false,
	"vue.codeActions.askNewComponentName": false,
	"vue.codeLens.enabled": false,
	"vue.splitEditors.icon": true,
	"vue.updateImportsOnFileMove.enabled": false,
	"vue.server.hybridMode": false,
	"vue.splitEditors.layout.right": [
		"script",
		"scriptSetup",
		"styles"
	],
	"vue.splitEditors.layout.left": [
		"template",
		"customBlocks"
	],
	"[vue]": {
		"editor.defaultFormatter": "Vue.volar"
	},
	"i18n-ally.editor.preferEditor": true,
	"i18n-ally.enabledParsers": [
		"yaml"
	],
	"i18n-ally.enabledFrameworks": [
		"vue",
		"vue-sfc"
	],
	// Nuxt
	"nuxtr.piniaFiles.defaultTemplate": "setup",
	"nuxtr.vueFiles.style.alwaysScoped": true,
	"nuxtr.vueFiles.style.defaultLanguage": "scss",
	// Markdown
	"markdown.preview.fontSize": 22,
	"markdown-toc.detectAndAutoSetSection": false,
	"markdown-toc.updateOnSave": false,
	"pandoc.docker.enabled": false,
	"pandoc.docxOptString": "-d pandoc",
	"pandoc.htmlOptString": "-d pandoc",
	"pandoc.pdfOptString": "-d pandoc",
	"pandoc.render.openViewer": false,
	"[markdown]": {
		"vscode-hanzi-counter.template.tooltipTemplateName": "zh-hans"
	},
	"markdown.extension.list.toggle.candidate-markers": [
		"-",
		"1."
	],
	// D2
	"D2.previewLayout": "tala",
	// Typst
	"typst-lsp.exportPdf": "never",
	// yaml
	"redhat.telemetry.enabled": true,
	"yaml.format.singleQuote": true,
	"[yaml][github-actions-workflow]": {
		"editor.tabSize": 2,
		"editor.insertSpaces": true,
		"editor.detectIndentation": false
	},
	// PHP
	"intelephense.stubs": [
		"Core",
		"ctype",
		"curl",
		"date",
		"hash",
		"imagick",
		"imap",
		"intl",
		"json",
		"mbstring",
		"meta",
		"openssl",
		"pcre",
		"PDO",
		"pdo_mysql",
		"Reflection",
		"session",
		"SPL",
		"standard",
		"superglobals",
		"Zend OPcache"
	],
	// SQL
	"sqlfluff.dialect": "mysql",
	"sqlfluff.linter.run": "onSave",
	// Python
	"ruff.lint.run": "onSave",
	"djlint.useVenv": false,
	"djlint.pythonPath": "/usr/local/lib/pipx/venvs/djlint/bin/python",
	"python.analysis.typeCheckingMode": "off",
	"python.analysis.autoImportCompletions": true,
	"notebook.lineNumbers": "on",
	"notebook.output.scrolling": true,
	"jupyter.disableJupyterAutoStart": false,
	"jupyter.pythonExportMethod": "commentMagics",
	"jupyter.allowUnauthorizedRemoteConnection": true,
	"jupyter.widgetScriptSources": [
		"jsdelivr.com",
		"unpkg.com"
	],
	"[python]": {
		"editor.tabSize": 4,
		"editor.insertSpaces": true,
		"editor.detectIndentation": false,
		"editor.defaultFormatter": "charliermarsh.ruff",
	},
	// "[django-html][handlebars][hbs][mustache][jinja][jinja-html][nj][njk][nunjucks][twig]": {
	//   "editor.defaultFormatter": "monosans.djlint"
	// },
	"autoDocstring.docstringFormat": "pep257",
	"autoDocstring.startOnNewLine": true,
	// Rust
	"rust-analyzer.server.path": "/usr/local/bin/rust-analyzer",
	"rust-analyzer.inlayHints.typeHints.enable": false,
	"rust-analyzer.inlayHints.closingBraceHints.enable": false,
	"rust-analyzer.inlayHints.chainingHints.enable": false,
	"rust-analyzer.inlayHints.parameterHints.enable": false,
	// Zig
	"zig.path": "zig",
	"zig.zls.path": "/home/bit0r/.config/Code/User/globalStorage/ziglang.vscode-zig/zls_install/zls",
	"zig.initialSetupDone": true,
	"zig.zls.enableInlayHints": false,
	// Java
	"boot-java.rewrite.reconcile": true,
	"java.compile.nullAnalysis.mode": "automatic",
	"java.format.comments.enabled": false,
	"java.sources.organizeImports.starThreshold": 8,
	"java.sources.organizeImports.staticStarThreshold": 4,
	"java.inlayHints.parameterNames.enabled": "none",
	"java.configuration.runtimes": [
		{
			"name": "JavaSE-1.8",
			"path": "/usr/lib/jvm/java-1.8.0-openjdk-amd64"
		},
		{
			"name": "JavaSE-11",
			"path": "/usr/lib/jvm/java-11-openjdk-amd64"
		},
		{
			"name": "JavaSE-17",
			"path": "/usr/lib/jvm/java-17-openjdk-amd64",
			"default": true
		}
	],
	"spring.dashboard.enableGutter": "off",
	"spring.dashboard.openWith": "external",
	"spring.initializr.serviceUrl": [
		"https://start.spring.io",
		"https://start.springboot.io"
	],
	// Android Reverse Engineering
	"apklab.apkSignerPath": "/opt/apk-signer/uber-apk-signer.jar",
	"apklab.apktoolPath": "/opt/apktool/apktool.jar",
	"apklab.jadxDirPath": "/opt/jadx/",
	"apklab.initProjectDirAsGit": false,
	"apklab.updateTools": false
