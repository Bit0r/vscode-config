{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "NodeJS",
      "program": "${file}",
      "outFiles": [
        "${workspaceFolder}/js/${fileBasenameNoExtension}.js"
      ],
      "cwd": "${workspaceFolder}",
      "internalConsoleOptions": "openOnSessionStart"
    }
  ]
}