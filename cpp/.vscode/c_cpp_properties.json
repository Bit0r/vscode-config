{
    "configurations": [
        {
            "name": "MinGW64",
            "defines": [
                "_DEBUG",
                "UNICODE"
            ],
            "cppStandard": "c++20",
            "compilerPath": "/usr/bin/g++",
            "intelliSenseMode": "gcc-x64",
            "browse": {
                "path": [
                    "${workspaceFolder}/src"
                ],
                "limitSymbolsToIncludedHeaders": true
            },
            "compilerArgs": [
                "-Wall",
                "-ggdb3",
                "-O0"
            ]
        }
    ],
    "version": 4
}