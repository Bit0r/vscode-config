{
    "configurations": [
        {
            "name": "Linux",
            "defines": [
                "_DEBUG",
                "UNICODE"
            ],
            "cStandard": "c18",
            "compilerPath": "/usr/bin/gcc",
            "intelliSenseMode": "gcc-x64",
            "browse": {
                "path": [
                    "${workspaceFolder}/src/"
                ],
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": ""
            },
            "compilerArgs": [
                "-Wall",
                "-ggdb3",
                "-O0"
            ]
        }
    ],
    "version": 4
}